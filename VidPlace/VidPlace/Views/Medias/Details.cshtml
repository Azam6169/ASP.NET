@model VidPlace.Models.Media
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>
    @Model.Name
</h2>

<ul>
    <li>Genre : @Model.Name</li>
    <li>Media Type : @Model.Name</li>
    <li>Date Released : @Model.ReleaseDate.ToShortDateString()</li>
    <li>Date Added : @Model.DateAdded.ToShortDateString()</li>
    <li>In Stock : @Model.NumberInStock</li>
    
</ul>



